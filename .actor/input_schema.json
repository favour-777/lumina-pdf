{
    "title": "Lumina PDF Input",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
      "fileUrls": {
        "title": "Document URLs",
        "type": "string",
        "description": "URLs to your documents (PDF, DOCX, EPUB, TXT, MD, HTML, RTF). One per line for batch processing.",
        "editor": "textarea",
        "example": "https://example.com/lecture.pdf\nhttps://example.com/notes.docx",
        "prefill": "https://example.com/document.pdf"
      },
      "outputFormats": {
        "title": "Output Formats",
        "type": "array",
        "description": "Select which study materials to generate",
        "editor": "select",
        "isMultiple": true,
        "default": ["cornellNotes", "flashcards", "quiz", "summary", "mindMap"],
        "items": {
          "type": "string",
          "enum": ["cornellNotes", "flashcards", "quiz", "summary", "mindMap"]
        },
        "uniqueItems": true
      },
      "numFlashcards": {
        "title": "Number of Flashcards",
        "type": "integer",
        "description": "How many flashcards to generate (5-100)",
        "minimum": 5,
        "maximum": 100,
        "default": 30,
        "editor": "number"
      },
      "numQuizQuestions": {
        "title": "Number of Quiz Questions",
        "type": "integer",
        "description": "How many quiz questions to generate (5-50)",
        "minimum": 5,
        "maximum": 50,
        "default": 20,
        "editor": "number"
      },
      "difficultyLevel": {
        "title": "Difficulty Level",
        "type": "string",
        "description": "Difficulty level for flashcards and quiz",
        "enum": ["easy", "medium", "hard", "mixed"],
        "default": "mixed",
        "editor": "select"
      }
    },
    "required": ["fileUrls"]
  }